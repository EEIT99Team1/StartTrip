<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="test.success" class="org.springframework.web.servlet.view.RedirectView">
		<property name="url" value="/testshow.jsp"></property>
		<property name="contextRelative" value="true"></property>
	</bean>

	<bean id="select" class="org.springframework.web.servlet.view.RedirectView">
		<property name="url" value="/page/search/02select.jsp"></property>
		<property name="contextRelative" value="true"></property>
	</bean>
	
	<bean id="playWallView" class="org.springframework.web.servlet.view.RedirectView">
		<property name="url" value="/page/backstage/managementPlaywall.jsp"></property>
		<property name="contextRelative" value="true"></property>
	</bean>
	
	<bean id="register.error" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/page/register/Register.jsp"></property>
	</bean>
	
	<bean id="register.success" class="org.springframework.web.servlet.view.RedirectView">
		<property name="url" value="/page/register/Success.jsp"></property>
		<property name="contextRelative" value="true"></property>
	</bean>
	
	<bean id="customerupdate.error" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/page/register/CutomerInfoAndUpdate.jsp"></property>
	</bean>
	
	<bean id="customerupdate.success" class="org.springframework.web.servlet.view.RedirectView">
		<property name="url" value="/page/CustomerInformation/CutomerInfoAndUpdate.jsp"></property>
		<property name="contextRelative" value="true"></property>
	</bean>
	
	<bean id="orderhistory" class="org.springframework.web.servlet.view.RedirectView">
		<property name="url" value="/page/register/OrderMessage.jsp"></property>
		<property name="contextRelative" value="true"></property>
	</bean>

	<bean id="order.error" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/page/search/04enter.jsp"></property>
	</bean>
	
	<bean id="order.success" class="org.springframework.web.servlet.view.RedirectView">
		<property name="url" value="/page/search/05check.jsp"></property>
		<property name="contextRelative" value="true"></property>
	</bean>
	<bean id="view.success" class="org.springframework.web.servlet.view.RedirectView">
		<property name="url" value="/page/search/07finally.jsp"></property>
		<property name="contextRelative" value="true"></property>
	</bean>
	
	<bean id="flightordercount" class="org.springframework.web.servlet.view.RedirectView">
		<property name="url" value="/page/backstage/statementsAna.jsp"></property>
		<property name="contextRelative" value="true"></property>
	</bean>

</beans>
